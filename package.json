    "@sanity/client": "^6.24.1",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^3.68.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "8.1.6",
